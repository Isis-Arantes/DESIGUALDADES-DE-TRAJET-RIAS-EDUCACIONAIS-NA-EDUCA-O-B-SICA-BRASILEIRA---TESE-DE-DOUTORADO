# PASSO 4


#apagando erros e duplicados 

brasil2007_v6 <- brasil2007_v5 %>% 
    filter(etapaEnsino!=9999) %>% 
    group_by(FK_COD_ALUNO) %>%
    add_count() %>% 
    filter(n == 1) %>%
    ungroup() %>% 
    select(-n)

