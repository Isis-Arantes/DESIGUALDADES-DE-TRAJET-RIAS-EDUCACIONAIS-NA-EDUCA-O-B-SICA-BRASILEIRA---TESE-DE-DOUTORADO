`%notin%` <- Negate('%in%')



df_original_dedup_1x3r <- df_original_1x3r %>% 
    filter(fk_cod_aluno %notin% Duplicados_1x3r$FK_COD_ALUNO)



write.csv2(df_original_dedup_1x3r, paste0(pastaSalvar, "df_original_dedup_1x3r.csv"), row.names = FALSE)
